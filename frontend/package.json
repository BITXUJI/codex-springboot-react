{
  "name": "codex-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=22 <23"
  },
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "build": "tsc -b && vite build --config vite.config.ts",
    "build:analyze": "tsc -b && ANALYZE=true vite build --config vite.config.ts",
    "preview": "vite preview --config vite.config.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install chromium",
    "test:e2e:headed": "playwright test --headed",
    "security:audit": "audit-ci --moderate",
    "verify:precommit": "npm run lint && npm test -- --watch=false && npm run build && npm run test:e2e",
    "generate": "openapi-typescript ../openapi/api.yml -o src/api/generated.ts"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "16.0.1",
    "@types/jest": "29.5.12",
    "@playwright/test": "1.58.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "@vitejs/plugin-react-swc": "4.2.3",
    "eslint": "8.57.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "openapi-typescript": "7.10.1",
    "audit-ci": "7.1.0",
    "rollup-plugin-visualizer": "6.0.5",
    "ts-jest": "29.2.5",
    "typescript": "5.9.3",
    "vite": "7.3.1"
  }
}
